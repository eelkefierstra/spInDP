<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_helling_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.aldert.spindpappandroidstudio22.HellingInfo"
    tools:showIn="@layout/activity_helling_info"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">

    <ProgressBar
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="199dp"
        android:id="@+id/HellingX"
        app:layout_constraintLeft_toLeftOf="@+id/content_helling_info"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintRight_toRightOf="@+id/content_helling_info"
        android:layout_marginEnd="16dp"
        tools:layout_constraintRight_creator="0"
        app:layout_constraintBottom_toBottomOf="@+id/content_helling_info"
        android:layout_marginBottom="352dp"
        tools:layout_constraintBottom_creator="0"
        app:layout_constraintHorizontal_bias="0.57" />

    <ProgressBar
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="255dp"
        android:id="@+id/HellingY"
        app:layout_constraintLeft_toLeftOf="@+id/HellingX"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toBottomOf="@+id/HellingX"
        android:layout_marginTop="40dp"
        tools:layout_constraintTop_creator="0" />

    <TextView
        android:text="X Afwijking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="199dp"
        android:id="@+id/textView"
        app:layout_constraintRight_toLeftOf="@+id/HellingX"
        android:layout_marginEnd="96dp"
        tools:layout_constraintRight_creator="0"
        app:layout_constraintBottom_toBottomOf="@+id/content_helling_info"
        android:layout_marginBottom="352dp"
        tools:layout_constraintBottom_creator="0" />

    <TextView
        android:text="Y afwijking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="255dp"
        android:id="@+id/textView3"
        app:layout_constraintLeft_toLeftOf="@+id/content_helling_info"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginTop="40dp"
        tools:layout_constraintTop_creator="0"
        app:layout_constraintRight_toLeftOf="@+id/HellingX"
        android:layout_marginEnd="96dp"
        tools:layout_constraintRight_creator="0"
        app:layout_constraintBottom_toBottomOf="@+id/HellingY"
        tools:layout_constraintBottom_creator="0"
        app:layout_constraintVertical_bias="1.0" />
</android.support.constraint.ConstraintLayout>
